{
  "name": "react-redux-bootstrap-webpack-ssr-starter",
  "version": "1.0.0",
  "description": "react js + redux + react router + server side rendering + hot reload + devTools + bootstrap + webpack starter",
  "main": "src/index.js",
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "dev": "cross-env NODE_ENV=dev webpack",
    "test": "cross-env NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prod": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "start": "cross-env NODE_ENV=dev node server.hot.reload",
    "serve-spa-dev": "npm run dev && nodemon ./src/server/SPA/index.js",
    "serve-spa--prod": "npm run prod && node ./src/server/SPA/index.js",
    "build-ssr": "cross-env NODE_ENV=production webpack -p --color --config webpack.server.ssr.config.js",
    "serve-ssr": "npm run build-ssr && node ./src/server/SSR/index.js"
  },
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "ES6",
    "ES2015",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools-extension",
    "devtools",
    "ssr",
    "server side",
    "universal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-redux-bootstrap-webpack-ssr-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-redux-bootstrap-webpack-ssr-starter/issues"
  },
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
